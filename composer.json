{
    "name": "speedwork/cache",
    "type": "library",
    "description": "Provides easy to use caching for Speedwork applications, built on top of the doctrine/cache package",
    "license": "MIT",
    "authors": [{
        "name": "Sankar Suda",
        "email": "sankar.suda@gmail.com"
    }],
    "require": {
        "php": ">=5.5",
        "doctrine/cache": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5",
        "satooshi/php-coveralls": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Speedwork\\Cache\\": "src"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
